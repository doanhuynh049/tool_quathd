# Add .bashrc
alias sudo='sudo_with_logging'

function sudo_with_logging() {
    if [[ $1 == "apt" && $2 == "install" ]]; then
        /usr/bin/sudo "$@"
        local status=$?
        if [ $status -eq 0 ]; then
            if ! grep  -q $3 ~/sudo_apt_install.log; then
                echo "Added package named $3 into log install..."
                echo "sudo  $@" >> ~/sudo_apt_install.log
            fi
        else
            echo "Installation of package $3 failed"
        fi
    else
    /usr/bin/sudo "$@"
    fi
}

